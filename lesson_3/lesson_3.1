from abc import ABC, abstractmethod


class Fountain(ABC):
    @abstractmethod
    def spray_water(self):
        pass


class SimpleFountain:
    def spray_water(self):
        print("Обычный фонтан: брызгает воду")


class MusicalFountain:
    def spray_water(self):
        print("Музыкальный фонтан: брызгает воду")


class LightedFountain:
    def spray_water(self):
        print("Цветной фонтан: брызгает воду")


fountains = [SimpleFountain(), MusicalFountain(), LightedFountain()]

for fountain in fountains:
    fountain.spray_water()